{
  "udp": {
    "port": 2013,
    "address": "127.0.0.1",
    "ipv6": false
  },
  "telemetron": {
    "transport": "api",
    "api": {
      "host": "api.telemetron.io",
      "port": "443",
      "protocol": "https",
      "path": "/tel/v2.0/metrics",
      "token": "",
      "timeout": 5000
    },
    "prefix": "telemetron",
    "flushInterval": 1000,
    "app": "telemetron-relay",
    "tags": {
      "env": "dev",
      "cluster": "local",
      "host": "localhost"
    }
  },
  "winston": {
    "silent": false,
    "level": "debug",
    "colorize": false,
    "timestamp": true,
    "prettyPrint": false,
    "showLevel": true,
    "debugStdout": true,
    "handleExceptions": true
  }
}
