{
  "carbon": {
    "tcp": {
      "port": 2013,
      "address": "127.0.0.1"
    },
    "udp": {
      "port": 2013,
      "address": "127.0.0.1",
      "ipv6": false
    }
  },
  "telemetronMetrics": {
    "host": "0.0.0.0",
    "port": 2013,
    "app": "carbon",
    "prefix": "telemetron",
    "flushInterval": 500,
    "tags": {
      "env": "dev",
      "cluster": "local",
      "host": "localhost"
    }
  },
  "collectd": {
    "port": 2014,
    "address": "localhost"
  },
  "tel": {
    "host": "tel.mindera",
    "port": 80,
    "url": {
      "accounts": "/tel/v2.0/accounts"
    },
    "refreshInterval": 60000
  },
  "influxdb": {
    "suffix": "",
    "flushInterval": 2000,
    "maxBufferSize": 50000
  },
  "winston": {
    "silent": false,
    "level": "debug",
    "colorize": false,
    "timestamp": true,
    "prettyPrint": false,
    "showLevel": true,
    "debugStdout": true,
    "handleExceptions": true
  }
}
